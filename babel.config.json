{
    "presets": [
        [
            "@babel/env",
            {
                "modules": false,
                "targets": ["last 2 versions", "not dead"]
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": ["@babel/plugin-proposal-class-properties", ["@babel/plugin-transform-runtime", {
        "version": "7.9.0"
      }]],
    "comments": false,
    "sourceMaps": true
}